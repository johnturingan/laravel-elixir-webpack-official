      "gulp"
   ],
   "author":"Jeffrey Way",
   "license":"MIT",
   "dependencies":{
      "buble":"^0.12.3",
      "buble-loader":"^0.2.2",
      "gulp-filter":"^4.0.0",
      "lodash":"^4.15.0",
      "webpack":"^1.14.0",
      "webpack-stream":"^3.2.0"
   },
   "devDependencies":{
      "babel-cli":"^6.10.1",
      "babel-preset-es2015":"^6.9.0"
   }
}
